  "meta": {
    "species": {
      "en_US": "Floating Sentry",
      "pt_BR": "Sentinela Flutuante",
      "de_DE": "Schwimmwache<<C<<Gepr\u00fcft: Minsu",
      "zh_CN": "\u6d6e\u7a7a\u54e8\u536b<<A<<[CHANGED 2019/09/11]",
      "zh_TW": "\u6d6e\u7a7a\u54e8\u885b<<A<<[CHANGED 2019/09/11]",
      "ja_JP": "\u98db\u884c\u30ed\u30dc\u30c3\u30c8<<A<<[CHANGED 2019/09/11]",
      "ko_KR": "\ubd80\uc720 \ud30c\uc218\ubcd1<<A<<[CHANGED 2019/09/11]",
      "langUid": 2
